#include<stdio.h>
int main()
{

 int j,i;
 for(i=1; i<=6; i++)
 {
   
      for(j=1; j<=5; j++)
      {
      if(((i==1 || i==4) &&(j>=2 && j<=4)) || (i>=2 &&(j==1 || j==5)))
      
          printf("*");
   
   else
   
     printf(" ") ;
   }
   printf("\n");
 }
}


(B)

#include<stdio.h>
int main()
{

 int j,i;
 for(i=1; i<=5; i++)
 {
   
      for(j=1; j<=4; j++)
      {
      if(((i==1 || i==5 ||i==3)&& j<4) || ((i==2 || i==4)&& j==4)|| j==1)
      
          printf(" *");
   
   else
   
     printf("  ") ;
   }
   printf("\n");
 }
}
 
 
 
 (c)
 
 #include<stdio.h>
int main()
{

 int j,i;
 for(i=1; i<=5; i++)
 {
   
      for(j=1; j<=4; j++)
      {
      if(i==1 || i==5 || j==1)
      
          printf(" *");
   
   else
   
     printf("  ") ;
   }
   printf("\n");
 }
}

(d)


#include<stdio.h>
int main()
{

 int j,i;
 for(i=1; i<=5; i++)
 {
   
      for(j=1; j<=5; j++)
      {
      if(i==1 || i==5 || j== 2|| j==5)
      
          printf("* ");
   
   else
   
     printf("  ") ;
   }
   printf("\n");
 }
}

